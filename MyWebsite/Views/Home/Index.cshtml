                                            document.getElementById('output4').innerHTML = "The word is Palindrome";
                                        }
                                        else {
                                            document.getElementById('output4').innerHTML = "The word is not Palindrome";
                                        }
                                    }

                                    function isPalindrome(str) {
                                        var len = str.length;
                                        for (var i = 0; i < Math.floor(len / 2) ; i++) {
                                            if (str[i] !== str[len - 1 - i]) {
