                                            var number = prompt("Please enter number", "");
                                            sum += parseFloat(number);
                                            product *= parseFloat(number);
                                            if (i == 0) {
                                                min = number;
                                                max = number;
                                            }
                                            if (number < min) {
                                                min = number;
                                            }
                                            if (number > max) {
                                                max = number;
                                            }
                                        }
                                        document.getElementById('output1').innerHTML = "Minimum: " + min + "<br />Maximum: " + max + "<br />Mean: " + sum / 5 + "<br />Sum: " + sum + "<br />Product: " + product;
                                    }

                                    ////////////////////

                                    function StartProgram2() {
                                        var num = parseInt(document.getElementById('txtnumber').value);
                                        var fact;
                                        if (num < 0) {
                                            fact = -1;
                                        }
                                            // If the number is 0, its factorial is 1.
                                        else if (num == 0) {
                                            fact = 1;
                                        }
                                        else {
                                            fact = num;
                                            while (num-- > 2) {
                                                fact *= num;
                                            }
                                        }
                                        document.getElementById('output2').innerHTML = "Factorial: " + fact;
                                    }
                                    ///////////////////////////////////

                                    function StartProgram3() {
                                        var num1 = parseInt(document.getElementById('txtnumber1').value);
