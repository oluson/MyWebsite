                                            }
                                            else {
                                                document.getElementById('output3').innerHTML += i + " <br />";
                                            }
                                        }
                                    }

                                    ///////////////////////////////////////

                                    function StartProgram4() {
                                        var word = document.getElementById('txtword').value.toLowerCase();
